{
	"name": "vscode-neovim",
	"displayName": "VSCode Neovim",
	"description": "Almost complete VIM integration by using Neovim (WIP)",
	"publisher": "asvetliakov",
	"extensionKind": "ui",
	"version": "0.0.3",
	"repository": {
		"type": "git",
		"url": "https://github.com/asvetliakov/vscode-neovim"
	},
	"bugs": {
		"url": "https://github.com/asvetliakov/vscode-neovim/issues",
		"email": "devpieron@gmail.com"
	},
	"engines": {
		"vscode": "^1.39.2"
	},
	"keywords": [
		"keybindings",
		"vi",
		"vim",
		"neovim"
	],
	"categories": [
		"Other",
		"Keymaps"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"configuration": {
			"title": "VSCode Neovim",
			"properties": {
				"vscode-neovim.neovimPath": {
					"type": "string",
					"default": "",
					"description": "Full path to Neovim executable"
				}
			}
		},
		"commands": [
			{
				"command": "vscode-neovim.escape",
				"title": "Escape key"
			},
			{
				"command": "vscode-neovim.cmdCompletion",
				"title": "Neovim: Trigger wildmenu completion"
			},
			{
				"command": "vscode-neovim.scrollUp",
				"title": "Neovim: scroll up"
			},
			{
				"command": "vscode-neovim.scrollDown",
				"title": "Neovim: scroll down"
			},
			{
				"command": "vscode-neovim.scrollHalfUp",
				"title": "Neovim: scroll half up"
			},
			{
				"command": "vscode-neovim.scrollHalfDown",
				"title": "Neovim: scroll half down"
			},
			{
				"command": "vscode-neovim.redo",
				"title": "Neovim: scroll half down"
			}
		],
		"keybindings": [
			{
				"command": "vscode-neovim.escape",
				"key": "Ctrl+c"
			},
			{
				"command": "vscode-neovim.cmdCompletion",
				"key": "tab",
				"when": "editorFocus && neovim.cmdLine"
			},
			{
				"command": "vscode-neovim.scrollUp",
				"key": "ctrl+b",
				"when": "editorTextFocus && neovim.mode != insert"
			},
			{
				"command": "vscode-neovim.scrollDown",
				"key": "ctrl+f",
				"when": "editorTextFocus && neovim.mode != insert"
			},
			{
				"command": "vscode-neovim.scrollHalfUp",
				"key": "ctrl+u",
				"when": "editorTextFocus && neovim.mode != insert"
			},
			{
				"command": "vscode-neovim.scrollHalfDown",
				"key": "ctrl+d",
				"when": "editorTextFocus && neovim.mode != insert"
			},
			{
				"command": "vscode-neovim.redo",
				"key": "ctrl+r",
				"when": "editorTextFocus && neovim.mode == normal"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/lodash": "^4.14.144",
		"@types/mocha": "^5.2.6",
		"@types/msgpack-lite": "^0.1.7",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.39.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"tslint": "^5.12.1",
		"typescript": "^3.6.4",
		"vscode-test": "^1.2.0"
	},
	"dependencies": {
		"lodash": "^4.17.15",
		"neovim": "^4.5.0"
	}
}
